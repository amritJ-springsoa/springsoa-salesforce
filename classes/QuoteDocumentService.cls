    public static String generateDocument(String htmlContent,Id quoteId){
        
        Quote quoteObj = [Select Id,OpportunityId from Quote where Id = :quoteId];
        quoteObj.Information__c = htmlContent;
        update quoteObj; 
        PageReference PDf =  Page.QuoteDocument_5;
        PDf.getParameters().put('Id',quoteId);
        PDf.setRedirect(true);

        Attachment attach = new Attachment();
        Blob b ;
        b = Test.isRunningTest() ? Blob.valueOf('MyString') : PDf.getContent();
        attach.Body = b;
        attach.Name = 'QuoteDocument_Service.pdf';
        attach.IsPrivate = false;
        attach.ParentId = quoteObj.OpportunityId;
        insert attach;
        return attach.Id+'';
    }
     
